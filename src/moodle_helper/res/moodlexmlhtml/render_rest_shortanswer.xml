{% for answer, fraction, feedback in q.answers %}
<answer fraction="{{ r.fragment(fraction) }}">
    <text><![CDATA[{{ r.fragment(answer) }}]]></text>
    <feedback>
        <text><![CDATA[{{ r.fragment(feedback) }}]]></text>
    </feedback>
</answer>
{% endfor %}
<usecase>{{ r.fragment(q.usecase) }}</usecase>
